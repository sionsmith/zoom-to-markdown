{
  "name": "zoom-meeting-notes",
  "version": "1.0.5",
  "description": "Automated tool to archive Zoom meeting recordings and transcripts as Markdown files in GitHub",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "vitest"
  },
  "keywords": [
    "zoom",
    "meeting-notes",
    "github-actions",
    "automation",
    "transcripts"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.8.6",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "gray-matter": "^4.0.3",
    "simple-git": "^3.27.0",
    "typescript": "^5.6.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "vitest": "^2.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
